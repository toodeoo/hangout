/* Pages/travel/preparation/index/index.wxss */
.tab-bar{
  width:100%;
  position: fixed;
  bottom:20px;
  left:50px;
  display: flex;
  padding-bottom: env(safe-area-inset-bottom);
}

.tab-bar-item{
  width: 20%;
  padding: 0;
  padding-left: 1%;
  margin: 0;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  height: 30%;
}

.default{
  font-weight: 550;
  font-size: 12px;
  color: #7B8B6F;
}

.active{
  font-size: 13px;
  font-weight: 900;
  color:black;
}

.tab-bar-item cover-image {
  position: relative;
  bottom: 25px;
  right: 10px;
  width: 40px;
  height: 35px;
  z-index: -1;
}

.hg{
  padding: 0;
  margin: 0;
  position: absolute;
  width: 50px;
  height: 130px;
  left: 62%;
  bottom: env(safe-area-inset-bottom);
}